@model ContohWeb.Models.Mahasiswa
@{
    ViewData["Title"] = "Details";
}
<h2>Detail Mahasiswa</h2>
<br />

<div class="col-md-6">
    <dl class="dl-horizontal">
        <dt>
            <label for="nim">Nim :</label>
        </dt>
        <dd>
            @Model.Nim
        </dd>
        <dt>
            <label for="nama">Nama :</label>
        </dt>
        <dd>
            @Model.Nama
        </dd>
        <dt>
            <label for="email">Email :</label>
        </dt>
        <dd>
            @Model.Email
        </dd>
        <dt>
            <label for="ipk">IPK :</label>
        </dt>
        <dd>
            @Model.IPK
        </dd>
    </dl>
</div>

