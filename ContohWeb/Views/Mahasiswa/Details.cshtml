@model ContohWeb.Models.Mahasiswa

@{
    ViewData["Title"] = "Details";
}

<h2>Detail Mahasiswa</h2>


    <div class="row">
        <div class="col-md-3">
            <label for="nim" class="pull-right">Nim :</label>
        </div>
        <div class="col-md-4">
            @Model.Nim
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <label for="nama" class="pull-right">Nama :</label>
        </div>
        <div class="col-md-4">
            @Model.Nama
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <label for="email" class="pull-right">Email :</label>
        </div>
        <div class="col-md-4">
            @Model.Email
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <label for="ipk" class="pull-right">IPK :</label>
        </div>
        <div class="col-md-4">
            @Model.IPK
        </div>
    </div>
   


